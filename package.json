{
  "name": "impress",
  "version": "0.1.244",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "description": "Impressive Multipurpose Application Server for Node.js",
  "keywords": [
    "application",
    "server",
    "impress",
    "api",
    "rpc",
    "soa",
    "service",
    "stateful",
    "rest",
    "restful",
    "web",
    "framework",
    "site",
    "template",
    "templating",
    "router",
    "routing",
    "proxy",
    "cluster",
    "cloud",
    "cache",
    "http",
    "https",
    "sse",
    "eventstream",
    "static",
    "websocket",
    "websockets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tshemsedinov/impress.git"
  },
  "main": "./lib/impress",
  "dependencies": {
    "async": "0.9.x",
    "colors": "1.0.x",
    "mkdirp": "0.5.x",
    "ncp": "2.0.x",
    "uglify-js": "2.4.x",
    "multiparty": "4.1.x",
    "iconv-lite": "0.4.x",
    "zip-stream": "0.5.x",
    "websocket": "1.0.x",
    "json-stringify-safe": "5.0.x",
    "csv": "0.4.x"
  },
  "devDependencies": {
    "mongodb": "1.4.x"
  },
  "engines": {
    "node": ">=0.10.6",
    "iojs": ">=1.0.0"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "install": "node install.js",
    "npm": "npm link npm"
  },
  "jshintConfig": {
    "predef": [ "api", "impress", "db", "global", "cms", "inArray", "emptyness", "window", "navigator", "document", "frames", "escape", "localStorage", "WebSocket", "XMLHttpRequest", "EventSource" ],
    "node": true,
    "indent": 2,
    "maxcomplexity": 25
  }
}